{
    "schema_version": 1,
    "quilt_loader": {
        "group": "${APEX_GROUP}.${TEST_MOD_ID}",
        "id": "${TEST_MOD_ID}",
        "version": "${TEST_MOD_VERSION}",
        "metadata": {
            "name": "${TEST_MOD_NAME}",
            "description": "${TEST_MOD_DESCRIPTION}",
            "contributors": {
                "Apex": "Founder & Lead Programmer",
                "FantasyGaming (Viscerus)": "Co-Founder & Artist",
                "RudySPG": "Web Developer, Supporter & Beta Tester",
                "TobiSPG": "Supporter & Beta Tester"
            },
            "contact": {
                "homepage": "https://apexstudios.dev/",
                "issues": "https://discord.apexstudios.dev",
                "sources": "https://github.com/ApexStudios-Dev/"
            },
            "icon": "logo.png"
        },
        "license": "GNU GENERAL PUBLIC LICENSE",
        "intermediate_mappings": "net.fabricmc:intermediary",
        "environment": "*",
        "entrypoints": {
            "init": [ "${APEX_GROUP}.${TEST_MOD_ID}.quilt.${TEST_MOD_NAME}Quilt::INSTANCE" ],
            "client_init": [ "${APEX_GROUP}.${TEST_MOD_ID}.quilt.${TEST_MOD_NAME}Quilt::INSTANCE" ],
            "server_init": [ "${APEX_GROUP}.${TEST_MOD_ID}.quilt.${TEST_MOD_NAME}Quilt::INSTANCE" ]
        },
        "depends": [
            {
                "id": "minecraft",
                "versions": "~${MINECRAFT_VERSION}"
            },
            {
                "id": "java",
                "versions": ">=${JAVA_VERSION}"
            },
            {
                "id": "quilt_loader",
                "versions": ">=${QUILT_LOADER_VERSION}"
            },
            {
                "id": "quilted_fabric_api",
                "versions": ">=${QUILT_FABRIC_VERSION}"
            },
            {
                "id": "${ARCHITECTURY_ID}",
                "versions": ">=${ARCHITECTURY_VERSION}"
            },
            {
                "id": "${CLOTH_CONFIG_FABRIC_ID}",
                "versions": ">=${CLOTH_CONFIG_VERSION_MOD}"
            },
            {
                "id": "${MOD_ID}",
                "versions": ">=${MOD_VERSION}"
            }
        ]
    },
    "mixin": [ "${TEST_MOD_ID}.mixins.json", "${TEST_MOD_ID}-common.mixins.json" ],
    "modmenu": {
        "modmenu.discord": "https://discord.apexstudios.dev/",
        "parent": "${MOD_ID}"
    }
}
